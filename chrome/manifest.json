{
  "name": "Sentinel Pub",
  "version": "0.1",
  "description": "Track the time you spend on websites",
  "icons": { "16": "images/icon16.png",
           "48": "images/icon48.png",
          "128": "images/icon128.png" },
  "background_page": "html/background.html",
  "content_scripts": [
    {
      "matches": ["http://events.santacruzsentinel.com/*"],
      "js": ["js/jquery.js", "js/pub.js"]
    }
  ],
  "permissions": [
    "<all_urls>"
  ]
}
